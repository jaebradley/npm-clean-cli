{
  "name": "npm-clean-cli",
  "version": "0.0.0-development",
  "description": "Clean NPM Dependencies",
  "main": "index.js",
  "bin": {
    "npmclean": "build/executables/npmclean.js"
  },
  "scripts": {
    "codecov": "codecov",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "compile": "babel -d build/ src/ --ignore node_modules,*.test.js",
    "lint": "eslint --ext .js .",
    "test": "jest --coverage",
    "prepublishOnly": "npm run compile",
    "semantic-commit": "commit",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jaebradley/npm-clean-cli.git"
  },
  "keywords": [
    "npm",
    "npm clean",
    "dependencies",
    "package.json",
    "cli"
  ],
  "author": "jae.b.bradley@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaebradley/npm-clean-cli/issues"
  },
  "homepage": "https://github.com/jaebradley/npm-clean-cli#readme",
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.0.2",
    "@commitlint/config-angular": "^6.0.2",
    "@commitlint/prompt": "^6.0.2",
    "@commitlint/prompt-cli": "^6.0.2",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "codecov": "^3.0.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "husky": "^0.14.3",
    "jest": "^22.0.6",
    "semantic-release": "^12.2.2"
  },
  "dependencies": {
    "chalk": "^2.3.0",
    "child-process-promise": "^2.2.1",
    "commander": "^2.13.0",
    "fs-extra": "^5.0.0",
    "inquirer": "^5.0.0",
    "rimraf-promise": "^2.0.0"
  }
}
